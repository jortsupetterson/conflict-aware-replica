{
  "name": "dacument",
  "version": "1.0.0",
  "description": "Schema-driven CRDT document with signed ops and role-based ACLs.",
  "keywords": [
    "crdt",
    "document",
    "collaboration",
    "replication",
    "sync",
    "acl",
    "signature",
    "crypto",
    "eventual-consistency",
    "offline"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test:only": "node --test test/*.test.js",
    "test": "npm run build && npm run test:only",
    "bench:crarray": "node bench/crarray.bench.js",
    "bench:crtext": "node bench/crtext.bench.js",
    "bench:crregister": "node bench/crregister.bench.js",
    "bench:crmap": "node bench/crmap.bench.js",
    "bench:crset": "node bench/crset.bench.js",
    "bench:crrecord": "node bench/crrecord.bench.js",
    "bench:all": "node bench/crarray.bench.js && node bench/crtext.bench.js && node bench/crregister.bench.js && node bench/crmap.bench.js && node bench/crset.bench.js && node bench/crrecord.bench.js",
    "bench": "npm run build && npm run bench:all",
    "sim:dacument:only": "node bench/dacument.sim.js",
    "sim:dacument": "npm run build && npm run sim:dacument:only",
    "verify": "npm run build && npm run test:only && npm run bench:all && npm run sim:dacument:only"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jortsupetterson/simple-crdts.git"
  },
  "bugs": {
    "url": "https://github.com/jortsupetterson/simple-crdts/issues"
  },
  "homepage": "https://github.com/jortsupetterson/simple-crdts#readme",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "bytecodec": "^2.1.0",
    "uuid": "^13.0.0",
    "zeyra": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
}
